{
    "manifest_version": 2,
    "name": "Basic Extension",
    "version": "1.0",
    "content_scripts": [
        {
        "matches": [
            "<all_urls>"
        ],
    
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "pic.png",
        "default_popup": "popup.html",
        "default_title": "I am a pop up!"
    }
}